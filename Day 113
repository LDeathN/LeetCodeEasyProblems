#First Problem
Array.prototype.last = function() {
    if (this.length === 0) {
        return -1;
    }
    return this[this.length - 1];
};



#Second Problem (2620)
var createCounter = function(n) {
    
    return function() {
        return n++;
    };
};



#Third Problem (2621)
async function sleep(millis) {
    return new Promise(resolve => setTimeout(resolve, millis));
}




