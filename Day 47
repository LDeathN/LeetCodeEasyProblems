#First Problem
class Solution(object):
    def canThreePartsEqualSum(self, arr):
        total_sum = sum(arr)
        if total_sum % 3 != 0:
            return False

        target = total_sum // 3
        current_sum, count = 0, 0

        for num in arr:
            current_sum += num
            if current_sum == target:
                count += 1
                current_sum = 0

        return count >= 3



#Second Problem
class Solution(object):
    def prefixesDivBy5(self, nums):
        result = []
        current_value = 0
        
        for bit in nums:
            current_value = (current_value << 1) + bit  
            result.append(current_value % 5 == 0) 
        
        return result




