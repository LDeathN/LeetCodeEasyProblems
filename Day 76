#First Problem
class Solution(object):
    def largestAltitude(self, gain):
        current = 0
        result = 0
        for i in range(len(gain)):
            current += gain[i]
            if current > result:
                result = current
        
        return result



#Second Problem
class Solution(object):
    def maximumTime(self, time):
        time = list(time)  

        if time[0] == '?':
            if time[1] == '?' or time[1] <= '3':
                time[0] = '2'
            else:
                time[0] = '1'

        if time[1] == '?':
            if time[0] == '2':
                time[1] = '3'
            else:
                time[1] = '9'

        if time[3] == '?':
            time[3] = '5'

        if time[4] == '?':
            time[4] = '9'

        return ''.join(time)



#Third Problem
SELECT 
    event_day AS day,
    emp_id,
    SUM(out_time - in_time) AS total_time
FROM 
    Employees
GROUP BY 
    event_day, 
    emp_id
ORDER BY 
    event_day, 
    emp_id;



#Fourth Problem
class Solution(object):
    def countBalls(self, lowLimit, highLimit):
        dictionary = {}
        result = 0
        for num in range(lowLimit, highLimit + 1):
            number = 0
            for digit in str(num):
                number += int(digit)
            if number not in dictionary:
                dictionary[number] = 0
            dictionary[number] += 1
        
        for key, value in dictionary.items():
            if value > result:
                result = value
        
        return result




