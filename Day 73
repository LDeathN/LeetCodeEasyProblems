#First Problem
class Solution(object):
    def maximumWealth(self, accounts):
        richest = 0
        for acc in accounts:
            if sum(acc) > richest:
                richest = sum(acc)

        return richest   



#Second Problem
class Solution(object):
    def interpret(self, command):
        command = command.replace("()", "o")
        command = command.replace("(", "")
        command = command.replace(")", "")

        return command



#Third Problem
SELECT tweet_id
FROM Tweets
WHERE LENGTH(content) > 15;



#Fourth Problem
class Solution(object):
    def countConsistentStrings(self, allowed, words):
        count = 0
        for word in words:
            check = True
            for letter in word:
                if letter not in allowed:
                    check = False
            if check:
                count += 1
        
        return count




