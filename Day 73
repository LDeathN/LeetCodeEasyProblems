#First Problem
class Solution(object):
    def maximumWealth(self, accounts):
        richest = 0
        for acc in accounts:
            if sum(acc) > richest:
                richest = sum(acc)

        return richest   



#Second Problem
class Solution(object):
    def interpret(self, command):
        command = command.replace("()", "o")
        command = command.replace("(", "")
        command = command.replace(")", "")

        return command




