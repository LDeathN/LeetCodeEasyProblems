#First Problem
class Solution(object):
    def findJudge(self, n, trust):
        if n == 1 and not trust:
            return 1

        trusts = [0] * (n + 1)
        trustedBy = [0] * (n + 1)

        for a, b in trust:
            trusts[a] += 1
            trustedBy[b] += 1

        for i in range(1, n + 1):
            if trusts[i] == 0 and trustedBy[i] == n - 1:
                return i

        return -1




