#First Problem (260)
class Solution(object):
    def singleNumber(self, nums):
        xor_result = 0
        for num in nums:
            xor_result ^= num
        
        diff_bit = xor_result & (-xor_result) 

        x, y = 0, 0
        for num in nums:
            if num & diff_bit:
                x ^= num 
            else:
                y ^= num 

        return [x, y]




