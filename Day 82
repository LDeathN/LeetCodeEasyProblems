#First Problem
class Solution(object):
    def subsetXORSum(self, nums):
        def all_subset_xors(nums):
            n = len(nums)
            total_sum = 0
            for subset in range(1 << n):
                current_xor = 0
                for i in range(n):
                    if subset & (1 << i):
                        current_xor ^= nums[i]
                total_sum += current_xor
            return total_sum

        return all_subset_xors(nums)




