#First  Problem
SELECT id, movie, description, rating
FROM Cinema
WHERE id % 2 = 1
  AND description != 'boring'
ORDER BY rating DESC;



#Second Problem
UPDATE Salary
SET sex = CASE 
             WHEN sex = 'm' THEN 'f'
             WHEN sex = 'f' THEN 'm'
          END;



#Third Problem
class Solution(object):
    def maximumProduct(self, nums):
        nums.sort()
        return max(nums[-1] * nums[-2] * nums[-3], nums[0] * nums[1] * nums[-1])




