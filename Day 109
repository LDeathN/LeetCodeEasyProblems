#First Problem (2525)
class Solution(object):
    def categorizeBox(self, length, width, height, mass):
        is_bulky = (length >= 10**4 or width >= 10**4 or height >= 10**4) or (length * width * height >= 10**9)

        is_heavy = (mass >= 100)

        if is_bulky and is_heavy:
            return "Both"
        elif is_bulky:
            return "Bulky"
        elif is_heavy:
            return "Heavy"
        else:
            return "Neither"



#Second Problem (2529)
class Solution(object):
    def maximumCount(self, nums):
        neg = 0
        pos = 0
        for num in nums:
            if num < 0:
                neg += 1
            elif num > 0:
                pos += 1
        
        return max(neg, pos)



#Third Problem (2535)
class Solution(object):
    def differenceOfSum(self, nums):
        digits = 0
        elements = 0
        for num in nums:
            elements += num
            for digit in str(num):
                digits += int(digit)

        return abs(digits - elements)




