#First Problem
class Solution(object):
    def specialArray(self, nums):
        nums.sort()  
        n = len(nums)
        
        for i in range(n):
            x = n - i  
            if nums[i] >= x and (i == 0 or nums[i - 1] < x):
                return x
        
        return -1 



#Second Problem
class Solution(object):
    def maxDepth(self, s):
        current_depth = 0
        max_depth = 0
        
        for char in s:
            if char == '(':
                current_depth += 1
                max_depth = max(max_depth, current_depth)
            elif char == ')':
                current_depth -= 1
                
        return max_depth




