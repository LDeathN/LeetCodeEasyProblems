#First Problem
class Solution(object):
    def check(self, nums):
        count_decreases = 0
        n = len(nums)
        
        for i in range(n):
            if nums[i] > nums[(i + 1) % n]:
                count_decreases += 1
                
            if count_decreases > 1:
                return False
        
        return True



#Second Problem
SELECT product_id
FROM Products
WHERE low_fats = "Y" AND recyclable = "Y";




