#First Problem
class Solution(object):
    def heightChecker(self, heights):
        expected = sorted(heights)
        count = 0
        for i in range(len(heights)):
            if expected[i] != heights[i]:
                count += 1

        return count



#Second Problem
SELECT
p.product_name,
s.year,
s.price
FROM Sales as s
JOIN Product as p
ON s.product_id = p.product_id




