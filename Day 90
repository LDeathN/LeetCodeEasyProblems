#First Problem
class Solution(object):
    def kthDistinct(self, arr, k):
        count = {}
        for string in arr:
            if string in count:
                count[string] += 1
            else:
                count[string] = 1

        distinct_strings = []
        for string in arr:
            if count[string] == 1:
                distinct_strings.append(string)

        if k <= len(distinct_strings):
            return distinct_strings[k-1]
        else:
            return ""



#Second Problem
class Solution(object):
    def smallestEqual(self, nums):
        for i in range(len(nums)):
            if i % 10 == nums[i]:
                return i
        return -1



#Third Problem
class Solution(object):
    def countVowelSubstrings(self, word):
        vowels = {'a', 'e', 'i', 'o', 'u'}
        n = len(word)
        count = 0

        for i in range(n):
            if word[i] in vowels:
                found_vowels = set()
                for j in range(i, n):
                    if word[j] in vowels:
                        found_vowels.add(word[j])
                        if len(found_vowels) == 5:
                            count += 1
                    else:
                        break

        return count




