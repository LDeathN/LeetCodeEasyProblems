#First Problem (2778)
class Solution(object):
    def sumOfSquares(self, nums):
        total = 0 
        for i in range(1, len(nums) + 1):
            if len(nums) % i == 0:
                total += nums[i-1] * nums[i-1]
        
        return total



#Second Problem (2784)
class Solution(object):
    def isGood(self, nums):
        n = max(nums) 
        base = list(range(1, n + 1)) + [n]  

        return sorted(nums) == sorted(base)




