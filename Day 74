#First Problem
SELECT
    date_id,
    make_name,
    COUNT(DISTINCT lead_id) AS unique_leads,
    COUNT(DISTINCT partner_id) AS unique_partners
FROM
    DailySales
GROUP BY
    date_id,
    make_name;



#Second Problem
class Solution(object):
    def reformatNumber(self, number):
        result = ''
        phone = []
        number = number.replace('-', '')
        number = number.replace(' ', '')
        for i in number:
            phone.append(i)
        while len(phone) > 4:
            for i in range(3):
                result += phone.pop(0)
            result += '-'
        if len(phone) == 4:
            while phone:
                result += phone.pop(0)
                result += phone.pop(0)
                result += '-'
                result += phone.pop(0)
                result += phone.pop(0)
        else:
            while phone:
                result += phone.pop(0)
        
        return result




