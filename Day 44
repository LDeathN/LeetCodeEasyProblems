#First Problem
class Solution(object):
    def validMountainArray(self, arr):
        n = len(arr)
    
        if n < 3:
            return False

        i = 1
        while i < n and arr[i] > arr[i - 1]:
            i += 1

        if i == 1 or i == n:
            return False
        while i < n and arr[i] < arr[i - 1]:
            i += 1

        return i == n



#Second Problem
class Solution(object):
    def diStringMatch(self, s):
        n = len(s)
        low, high = 0, n
        perm = []

        for char in s:
            if char == 'I':
                perm.append(low)
                low += 1
            else: 
                perm.append(high)
                high -= 1

        perm.append(low)

        return perm




