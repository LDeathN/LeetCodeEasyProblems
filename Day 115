#First Problem (2648)
var fibGenerator = function*() {
    let a = 0, b = 1;
    while (true) {
        yield a;
        [a, b] = [b, a + b];
    }
};



#Second Problem (2651)
class Solution(object):
    def findDelayedArrivalTime(self, arrivalTime, delayedTime):
        return (arrivalTime + delayedTime) % 24



#Third Problem (2652)
class Solution(object):
    def sumOfMultiples(self, n):
        total = 0
        for i in range(n + 1):
            if i % 3 == 0 or i % 5 == 0 or i % 7 == 0:
                total += i
        
        return total



#Fourth Problem (2656)
class Solution(object):
    def maximizeSum(self, nums, k):
        nums = sorted(nums)
        biggest = nums.pop()
        total = 0
        for i in range(k):
            total += biggest
            biggest += 1
        
        return total




