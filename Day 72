#First Problem
class OrderedStream:
    def __init__(self, n):
        self.stream = [None] * n
        self.ptr = 0

    def insert(self, idKey, value):
        self.stream[idKey - 1] = value

        chunk = []
        while self.ptr < len(self.stream) and self.stream[self.ptr] is not None:
            chunk.append(self.stream[self.ptr])
            self.ptr += 1
        
        return chunk




