#First Problem
class Solution(object):
    def sortArrayByParityII(self, nums):
        even = []
        odd = []
        result = []
        for num in nums:
            if num % 2 == 0:
                even.append(num)
            else:
                odd.append(num)
        while even or odd:
            result.append(even.pop())
            result.append(odd.pop())
        
        return result



#Second Problem
class Solution(object):
    def isLongPressedName(self, name, typed):
        i = 0
        j = 0
        while j < len(typed):
            if i < len(name) and name[i] == typed[j]:
                i += 1
            elif j == 0 or typed[j] != typed[j - 1]:
                return False
            j += 1
        return i == len(name)




