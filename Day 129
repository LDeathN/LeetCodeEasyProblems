#First Problem (2996)
class Solution(object):
    def missingInteger(self, nums):
        longest_seq_sum = nums[0]
        for i in range(1, len(nums)):
            if nums[i] == nums[i - 1] + 1:
                longest_seq_sum += nums[i]
            else:
                break

        x = longest_seq_sum
        nums_set = set(nums)

        while x in nums_set:
            x += 1
        
        return x




