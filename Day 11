#First Problem
SELECT email
FROM Person
GROUP BY email
HAVING COUNT(email) > 1;



#Second Problem
SELECT c.name as Customers
FROM Customers as c
LEFT JOIN Orders as o
ON c.id = o.customerId
WHERE o.id IS NULL;



#Third Problem
class Solution:
    # @param n, an integer
    # @return an integer
    def reverseBits(self, n):
        result = 0
        for _ in range(32):
            result = (result << 1) | (n & 1)
            n >>= 1
        return result




